{
  "ConnectionStrings": {
    "EHRConnectionString": "<YOUR_SQL_SERVER_CONNECTION_STRING>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "<YOUR_JWT_SECRET_KEY>",
    "Issuer": "<YOUR_JWT_ISSUER>",
    "Audience": "<YOUR_JWT_AUDIENCE>",
    "Subject": "<YOUR_JWT_SUBJECT>",
    "DurationInMinutes": 60
  },
  "AllowedHosts": "*",
  "Smtp": {
    "Host": "smtp.gmail.com",
    "Port": 587,
    "EnableSsl": true,
    "SenderName": "<YOUR_SENDER_NAME>",
    "Email": "<YOUR_EMAIL>",
    "Password": "<YOUR_EMAIL_PASSWORD>",
    "From": "<YOUR_EMAIL>"
  },
  "Stripe": {
    "SecretKey": "<YOUR_STRIPE_SECRET_KEY>"
  }
}

